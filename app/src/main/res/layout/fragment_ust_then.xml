<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:label="400 Shades of UST"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.MyApp.prototype_tan.UstThen$PlaceholderFragment"
    android:background="@drawable/abc_ab_bottom_solid_dark_holo">


    <Button
        style="?android:attr/buttonStyleSmall"
        android:onClick="sixty"
        android:layout_width="500dp"
        android:layout_height="50dp"
        android:text="1960's"
        android:id="@+id/button"
        android:clickable="true"
        android:background="#ffff3c00"
        android:textColor="#fffff50b"
        android:textSize="20dp"
        android:layout_above="@+id/button2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:onClick="seventy"
        android:layout_width="500dp"
        android:layout_height="50dp"
        android:text="1970's"
        android:id="@+id/button2"
        android:clickable="true"
        android:textColor="#fffff50b"
        android:background="#ffff3c00"
        android:layout_above="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:onClick="eighty"
        android:layout_width="500dp"
        android:layout_height="50dp"
        android:text="1980's"
        android:id="@+id/button3"
        android:clickable="true"
        android:textColor="#fffff50b"
        android:background="#ffff3c00"
        android:layout_above="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:onClick="ninety"
        android:layout_width="500dp"
        android:layout_height="50dp"
        android:text="1990's"
        android:id="@+id/button4"
        android:clickable="true"
        android:textColor="#fffff50b"
        android:background="#ffff3c00"
        android:layout_above="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:onClick="thousand"
        android:layout_width="500dp"
        android:layout_height="50dp"
        android:text="2000's"
        android:id="@+id/button5"
        android:clickable="true"
        android:textColor="#fffff50b"
        android:background="#ffff3c00"
        android:layout_marginBottom="75dp"
        android:textSize="20dp"
        android:singleLine="false"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
