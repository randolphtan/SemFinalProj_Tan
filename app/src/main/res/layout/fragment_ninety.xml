<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:label="400 Shades of UST"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.MyApp.prototype_tan.ninety$PlaceholderFragment"
    android:background="@drawable/abc_ab_bottom_solid_dark_holo">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="THROWBACK 1990s"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="30dp"
        android:textColor="#ffffca00" />

    <ImageView
        android:layout_width="800dp"
        android:layout_height="300dp"
        android:id="@+id/imageView"
        android:src="@drawable/delarosa"
        android:layout_marginRight="168dp"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />

    <TextView
        android:layout_width="160dp"
        android:layout_height="200dp"
        android:id="@+id/editText"
        android:text="Rolando de la Rosa during his second term as Rector, 1995.  De la Rosa was the former Rector Magnificus of UST. He served from 1990-1994, 1994-1998, and from 2007-2012 for a total of 13 years. "
        android:textColor="#ffacacac"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_marginBottom="26dp" />

    <ImageView
        android:onClick="home4"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:id="@+id/imageView3"
        android:clickable="true"
        android:src="@drawable/hehehemain"
        android:layout_marginLeft="111dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/editText"
        android:layout_alignStart="@+id/editText" />

</RelativeLayout>
